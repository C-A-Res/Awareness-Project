<grammar xmlns="http://www.w3.org/2001/06/grammar" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.w3.org/2001/06/grammar          http://www.w3.org/TR/speech-grammar/grammar.xsd" xml:lang="en-US" version="1.0" tag-format="semantics-ms/1.0" root="Top">
  <rule id="Top">
    <one-of>
      <item>
        <ruleref uri="#Greeting" />
      </item>
      <item>
        <ruleref uri="#Interjection" />
      </item>
      <item>
        <ruleref uri="#Question" />
      </item>
      <item>
        <ruleref uri="#Command" />
      </item>
      <item>
        <ruleref uri="#PersonReference" />
      </item>
      <item>
        <ruleref uri="#EasterEggs" />
      </item>
    </one-of>
  </rule>
  <rule id="Command">
    <one-of>
      <item>Reload grammars</item>
    </one-of>
  </rule>
  <rule id="EasterEggs">
    <one-of>
      <item>Beam me up, Scotty!</item>
      <item>May the force be with you, <ruleref uri="#PersonReference" /></item>
      <item>I've; see-een, thingss, you people wouldn't belee-eeve.</item>
      <item>Attack ships on fire off the shoulder of Orion.</item>
      <item>I watched C beams glitter in the dark near the Tannh√§user Gate.</item>
      <item>All those moments will be lost in time, like tears in rain</item>
      <item>Dead or alive you're coming with me</item>
    </one-of>
  </rule>
  <rule id="Greeting">
    <one-of>
      <item>Hello</item>
      <item>Greetings</item>
      <item>Good morning</item>
      <item>What's Sup</item>
      <item>Sup</item>
    </one-of>
  </rule>
  <rule id="Interjection">
    <one-of>
      <item>okay</item>
      <item>hm</item>
      <item>um</item>
      <item>ah</item>
      <item>cool</item>
      <item>huh?</item>
      <item>wow!</item>
      <item>huck you</item>
    </one-of>
  </rule>
  <rule id="Question">
    <one-of>
      <item>
        <ruleref uri="#WhoQuestion" />
      </item>
      <item>
        <ruleref uri="#WhatQuestion" />
      </item>
      <item>
        <ruleref uri="#WhenQuestion" />
      </item>
      <item>
        <ruleref uri="#WhereQuestion" />
      </item>
    </one-of>
  </rule>
  <rule id="WhereQuestion">
    <one-of>
      <item>
        Where is <ruleref uri="#location" /></item>
      <item>
        Where can I find <ruleref uri="#location" /></item>
      <item>Where am I</item>
      <item>
        Where does <ruleref uri="#PersonReference" /> work
      </item>
      <item>
        Can you tell me where <ruleref uri="#PersonReference" /> is
      </item>
      <item>
        Where is <ruleref uri="#EventName" /></item>
    </one-of>
  </rule>
  <rule id="location">
    <one-of>
      <item>the bathroom</item>
      <item>something to eat</item>
      <item>
        <ruleref uri="#PersonReference" />'s office
      </item>
      <item>
        Ford <ruleref uri="#OfficeNumber" /></item>
    </one-of>
  </rule>
  <rule id="WhenQuestion">
    <one-of>
      <item>
        Can you tell me when <ruleref uri="#EventName" /> is
      </item>
      <item>
        When does <ruleref uri="#EventName" /> meet
      </item>
      <item>
        When is <ruleref uri="#EventName" /></item>
    </one-of>
  </rule>
  <rule id="WhatQuestion">
    <one-of>
      <item>What time is it</item>
      <item>What is going on today</item>
      <item>
        What is <ruleref uri="#PersonReference" />'s email
      </item>
      <item>
        Can you tell me what <ruleref uri="#PersonReference" />'s email is
      </item>
      <item>What do I have today</item>
      <item>What talks are today</item>
    </one-of>
  </rule>
  <rule id="WhoQuestion">
    <one-of>
      <item>
        Who is <ruleref uri="#PersonReference" /></item>
      <item>
        Can you tell me who <ruleref uri="#PersonReference" /> is
      </item>
    </one-of>
  </rule>
  <rule id="EventName">
    <one-of>
      <item>
        <ruleref uri="#Courses" />
      </item>
      <item>the colloquium</item>
      <item>today's talk</item>
      <item>the faculty meeting</item>
      <item>Intro to AI</item>
      <item>214</item>
    </one-of>
  </rule>
  <rule id="Courses">
    <one-of>
      <item>
        <ruleref uri="#Courses_name" />
      </item>
      <item>
        <ruleref uri="#Courses_id" />
      </item>
    </one-of>
  </rule>
  <rule id="Courses_name">
    <one-of>
      <item>EECS four ninty five Companion Studio</item>
      <item>Companion Studio</item>
    </one-of>
  </rule>
  <rule id="Courses_id">
    <one-of>
      <item>
        EECS <ruleref uri="#OneToNine" /><ruleref uri="#ZeroToNinetyNine" /></item>
    </one-of>
  </rule>
