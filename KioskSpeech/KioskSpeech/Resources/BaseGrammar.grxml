  <rule id="InquiryStarter">
    <one-of>
      <item>
        Do you
        <item repeat="0-1">happen to</item>
        <one-of>
          <item>have</item>
          <item>know</item>
        </one-of>
      </item>
      <item>
        <one-of>
          <item>Can</item>
          <item>Could</item>
          <item>Would</item>
        </one-of>
        you tell me
      </item>
    </one-of>
  </rule>

  <rule id="InquisitionOrderStarter">
    <one-of>
      <item>Show me</item>
      <item>Tell me</item>
    </one-of>
  </rule>

  <rule id="PersonInquiryContent">
    <one-of>
      <item>
        where I can find<ruleref uri="#PersonReference"/>
      </item>
      <item>
        where to find<ruleref uri="#PersonReference"/>
      </item>
      <item>
        where <ruleref uri="#PersonReference"/> is
      </item>
      <item>
        the professor for <ruleref uri="#PersonReference"/> is
      </item>
    </one-of>
  </rule>

  <rule id="ObjectInquiryContent">
    <one-of>
      <item>
        <one-of>
          <item>about</item>
          <item>if there is</item>
          <item>the time for</item>
          <item>the host of</item>
        </one-of>
        <item repeat="0-1">
          <one-of>
            <item>a</item>
            <item>the</item>
          </one-of>
        </item>
        <ruleref uri="#EventMaybeTemporallyStampped"/>
      </item>
      <item>
        <ruleref uri="#ObjectPossessedByPerson"/>
        <item repeat="0-1">
          <one-of>
            <item>lying around</item>
            <item>with you</item>
          </one-of>
        </item>
      </item>
      <item>
        <one-of>
          <item>if</item>
          <item>whether</item>
        </one-of>
        <ruleref uri="#Course"/>
        is offered
        <item repeat="0-1">
          by
          <item repeat="0-1">
            <ruleref uri="#PersonReference"/>
          </item>
        </item>
        <item repeat="0-1">
          <ruleref uri="#QuarterReference"/>
        </item>
      </item>
      <item>
        <one-of>
          <item>when</item>
          <item>where</item>
        </one-of>
        <ruleref uri="#EventMaybeTemporallyStampped"/>
        is
      </item>
      <item>
        the name of the professor for
        <ruleref uri="#Course"/>
        <item repeat="0-1">
          <ruleref uri="#QuarterReference"/>
        </item>
      </item>
      <item>
        <one-of>
          <item>the professor who</item>
          <item>the name of the professor who</item>
          <item>who</item>
          <item>
            <one-of>
              <item>whether</item>
              <item>if</item>
            </one-of>
            <ruleref uri="#PersonReference"/>
            is
          </item>
        </one-of>
        <one-of>
          <item>teaching</item>
          <item>the professor teaching</item>
          <item>the person teaching</item>
          <item>the professor for</item>
          <item>the professor for teaching</item>
          <item>the person for</item>
        </one-of>
        <ruleref uri="#Course"/>
        <item repeat="0-1">
          <ruleref uri="#QuarterReference"/>
        </item>
      </item>
      <item>
        <one-of>
          <item>the professor for</item>
          <item>the professor who is teaching</item>
          <item>the professor teaching</item>
          <item>who lectures</item>
          <item>who teaches</item>
        </one-of>
        <ruleref uri="#Course"/>
        <item repeat="0-1">
          <ruleref uri="#QuarterReference"/>
        </item>
      </item>
      <item>
        <one-of>
          <item>the host for</item>
          <item>the host of</item>
          <item>the time for</item>
          <item>the time of</item>
        </one-of>
        <ruleref uri="#EventName"/>
        <item repeat="0-1">
          <ruleref uri="#QuarterReference"/>
        </item>
