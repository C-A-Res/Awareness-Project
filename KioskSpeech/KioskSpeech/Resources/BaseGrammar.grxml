  <!-- Numbers -->
  <rule id="Three_digit_number">
    <one-of>
      <item>
        <ruleref uri="#OneToNine"/> hundred
      </item>
      <item>
        <ruleref uri="#OneToNine"/> o <ruleref uri="#OneToNine"/>
      </item>
      <item>
        <ruleref uri="#OneToNine"/>
        <ruleref uri="#TenToNinetyNine"/>
      </item>
    </one-of>
  </rule>
  <rule id="OfficeNumber">
    <ruleref uri="#OneToNine"/>
    <ruleref uri="#Three_digit_number"/>
  </rule>
  <rule id="ZeroToNinetyNine">
    <one-of>
      <item>o</item>
      <item>0</item>
      <item>
        <ruleref uri="#OneToNine"/>
      </item>
      <item>
        <ruleref uri="#TenToNineteen"/>
      </item>
      <item>
        <ruleref uri="#TwentyToNinety"/>
        <item repeat="0-1">
          <ruleref uri="#OneToNine"/>
        </item>
      </item>
    </one-of>
  </rule>
  <rule id="TenToNinetyNine">
    <one-of>
      <item>
        <ruleref uri="#TenToNineteen"/>
      </item>
      <item>
        <ruleref uri="#TwentyToNinety"/>
        <item repeat="0-1">
          <ruleref uri="#OneToNine"/>
        </item>
      </item>
    </one-of>
  </rule>
  <rule id="OneToNine">
    <one-of>
      <item>1</item>
      <item>2</item>
      <item>3</item>
      <item>4</item>
      <item>5</item>
      <item>6</item>
      <item>7</item>
      <item>8</item>
      <item>9</item>
    </one-of>
  </rule>
  <rule id="OneToTwentyThree">
    <one-of>
      <item>
        <ruleref uri="#OneToNine"/>
      </item>
      <item>
        <ruleref uri="#TenToNineteen"/>
      </item>
      <item>
        twenty
        <item repeat="0-1">
          <one-of>
            <item>1</item>
            <item>2</item>
            <item>3</item>
          </one-of>
        </item>
      </item>
      <item>zero</item>
    </one-of>
  </rule>
  <rule id="TenToNineteen">
    <one-of>
      <item>10</item>
      <item>11</item>
      <item>12</item>
      <item>13</item>
      <item>14</item>
      <item>15</item>
      <item>16</item>
      <item>17</item>
      <item>18</item>
      <item>19</item>
    </one-of>
  </rule>
  <rule id="TwentyToNinety">
    <one-of>
      <item>20</item>
      <item>30</item>
      <item>40</item>
      <item>50</item>
      <item>60</item>
      <item>70</item>
      <item>80</item>
      <item>90</item>
    </one-of>
  </rule>

