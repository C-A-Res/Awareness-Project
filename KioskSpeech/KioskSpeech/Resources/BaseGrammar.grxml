<grammar xmlns="http://www.w3.org/2001/06/grammar"
			xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
			xsi:schemaLocation="http://www.w3.org/2001/06/grammar 
								http://www.w3.org/TR/speech-grammar/grammar.xsd"
			xml:lang="en-US" version="1.0"
			tag-format="semantics-ms/1.0"
			root="Top">

  <!-- mark -->
  <rule id="Top">
    <one-of>
      <item><ruleref uri="#Question"/></item>
      <item><ruleref uri="#Greeting"/></item>
      <item><ruleref uri="#WrapUp"/></item>
      <item><ruleref uri="#Interjection"/></item>
      <item><ruleref uri="#Question"/></item>
      <item><ruleref uri="#Command"/></item>
      <item><ruleref uri="#EasterEggs"/></item>
    </one-of>
  </rule>

  <!-- mark -->
  <rule id="Question">
    <one-of>
      <item><ruleref uri="#AreQuestion"/></item>
      <item><ruleref uri="#DidQuestion"/></item>
      <item><ruleref uri="#DoesQuestion"/></item>
      <item><ruleref uri="#HasQuestion"/></item>
      <item><ruleref uri="#IsQuestion"/></item>
      <item><ruleref uri="#Inquisition"/></item>
      <item><ruleref uri="#InquisitionOrder"/></item>
      <item><ruleref uri="#HowQuestion"/></item>
      <item><ruleref uri="#WhatQuestion"/></item>
      <item><ruleref uri="#WhenQuestion"/></item>
      <item><ruleref uri="#WhereQuestion"/></item>
      <item><ruleref uri="#WhichQuestion"/></item>
      <item><ruleref uri="#WhoQuestion"/></item>
      <item><ruleref uri="#WillQuestion"/></item>
    </one-of>
  </rule>

  <!-- mark -->
  <rule id="AreQuestion">
    Are there
    <item>
      <item repeat="0-1">
        any
        <one-of>
          <item>good</item>
          <item>short</item>
        </one-of>
      </item>
      <ruleref uri="#EventMaybeTemporallyStampped"/>
    </item>
    <item repeat="0-1">
      <one-of>
        <item>
          <ruleref uri="#DurationComparison"/>
        </item>
        <item>
          that are <ruleref uri="#DurationComparison"/>
        </item>
        <item>that are short</item>
        <item>that I should go to</item>
      </one-of>
    </item>
    <item repeat="0-1">
      <ruleref uri="#TemporalReference"/>
    </item>
  </rule>

  <rule id="DidQuestion">
    Did
    <ruleref uri='#PersonReference'/>
    go to
    <ruleref uri='#School'/>
    <item repeat="0-1">
      for
      <item repeat="0-1">
        <ruleref uri="#PossessivePronoun"/>
      </item>
      <ruleref uri="#DegreeLevel"/>
    </item>
  </rule>

  <rule id="DoesQuestion">
    Does
    <one-of>
      <item>
        <ruleref uri="#EventMaybeTemporallyStampped"/>
        meet
        <item repeat="0-1">in the usual place</item>
        <item repeat="0-1">
          <one-of>
            <item>
              <ruleref uri="#TemporalReference"/>
            </item>
            <item>
              on<ruleref uri="#Date"/>
            </item>
            <item>
              <one-of>
                <item>on</item>
                <item>this</item>
                <item>next</item>
              </one-of>
              <ruleref uri="#DayOfTheWeek"/>
            </item>
          </one-of>
        </item>
      </item>
      <item>
        <ruleref uri="#PersonReference"/>
        <one-of>
          <item>
            do research in
            <ruleref uri="#AcademicDiscipline"/>
          </item>
          <item>
            teach
            <ruleref uri="#Course"/>
            <item repeat="0-1">
              <ruleref uri="#QuarterReference"/>
            </item>
          </item>
          <item>have an office here</item>
          <item>work here</item>
        </one-of>
      </item>
      <item>
        <ruleref uri="#EventName"/>start now
      </item>
    </one-of>
  </rule>

  <rule id="HasQuestion">
    <item>
      Has
      <ruleref uri="#PersonReference"/>
      listed his
      <ruleref uri="#Email"/>
    </item>
  </rule>

  <rule id="IsQuestion">
    Is
    <one-of>
      <item>
        <ruleref uri="#EventMaybeTemporallyStampped"/>
        <one-of>
          <item>
            hosted by<ruleref uri="#PersonReference"/>
          </item>
          <item>
            <ruleref uri="#DurationComparison"/>
          </item>
        </one-of>
      </item>
      <item>
        <ruleref uri="#EventMaybeTemporallyStampped"/>
        still
        <one-of>
          <item>happening</item>
          <item>going on</item>
          <item>on</item>
        </one-of>
      </item>
      <item>
        <ruleref uri="#EventMaybeTemporallyStampped"/>at<ruleref uri="#TimeStamp"/>
      </item>
      <item>
        <ruleref uri="#EventMaybeTemporallyStampped"/>in<ruleref uri="#Place"/>
      </item>
      <item>
        <ruleref uri="#PersonReference"/>
        <one-of>
          <item>
            <ruleref uri="#EnglishArticle"/>
            <ruleref uri="#AcademicDiscipline"/>
            <one-of>
              <item>faculty</item>
              <item>professor</item>
            </one-of>
          </item>
          <item>
            in
            <item repeat="0-1">the</item>
            <ruleref uri="#AcademicDiscipline"/>
            <item repeat="0-1">group</item>
          </item>
          <item>
            teaching
            <item repeat="0-1">the</item>
            <ruleref uri="#Course"/>
            <ruleref uri="#QuarterReference"/>
          </item>
          <item>
            an
            <ruleref uri="#Alumn"/>
            of
            <ruleref uri="#School"/>
          </item>
          <item>
            's office
            <one-of>
              <item>
                <one-of>
                  <item>in</item>
                  <item>on</item>
                </one-of>
                <ruleref uri="#SpecificPlace"/>
              </item>
              <item>
                <ruleref uri="#LocationReference"/>
              </item>
            </one-of>
          </item>
        </one-of>
      </item>
      <item>
        <ruleref uri="#School"/>
        where
        <ruleref uri="#PersonReference"/>
        <one-of>
          <item>
            <item>got</item>
            <ruleref uri="#PossessivePronoun"/>
            <ruleref uri="#DegreeLevel"/>
          </item>
          <item>
            went <item repeat="0-1">to</item>for<ruleref uri="#SchoolLevel"/>
          </item>
        </one-of>
      </item>
      <item>
        there
        <item repeat="0-1">
          <one-of>
            <item>a</item>
            <item>an</item>
            <item>any</item>
          </one-of>
        </item>
        <one-of>
          <item>
            <ruleref uri="#Email"/>
            of
            <ruleref uri="#PersonReference"/>
            <item repeat="0-1">available</item>
          </item>
          <item>
            <item repeat="0-1">invited</item>
            <ruleref uri="#EventMaybeTemporallyStampped"/>
          </item>
          <item>
            <ruleref uri="#Course"/>
            <item repeat="0-1">available</item>
            <ruleref uri="#QuarterReference"/>
          </item>
          <item>
            discussion session for
            <ruleref uri="#EventMaybeTemporallyStampped"/>
          </item>
        </one-of>
      </item>
    </one-of>
  </rule>

  <rule id="Inquisition">
    <ruleref uri="#InquiryStarter"/>
    <one-of>
      <item>
        <ruleref uri="#ObjectInquiryContent"/>
      </item>
      <item>
        <ruleref uri="#PersonInquiryContent"/>
      </item>
    </one-of>
  </rule>

  <rule id="InquisitionOrder">
    <ruleref uri="#InquisitionOrderStarter"/>
    <one-of>
      <item>
        <ruleref uri="#ObjectInquiryContent"/>
      </item>
      <item>
        <ruleref uri="#PersonInquiryContent"/>
      </item>
    </one-of>
  </rule>

  <rule id="HowQuestion">
    How
    <one-of>
      <item>
        <one-of>
          <item>do</item>
          <item>can</item>
          <item>could</item>
          <item>may</item>
        </one-of>
        <one-of>
          <item>I</item>
          <item>We</item>
        </one-of>
        <one-of>
          <item>get to</item>
          <item>find</item>
          <item>reach</item>
        </one-of>
        <ruleref uri="#LocationOrPerson"/>
      </item>
      <item>
        long is
        <ruleref uri="#EventMaybeTemporallyStampped"/>
      </item>
      <item>
        many <ruleref uri="#EventName"/> are
        <item repeat="0-1">there</item>
        <ruleref uri="#TemporalReference"/>
      </item>
    </one-of>
  </rule>

  <rule id="WhatQuestion">
    what
    <one-of>
      <item>
        is
        <one-of>
          <item>going on today</item>
          <item>
            <ruleref uri="#ObjectPossessedByPerson"/>
          </item>
          <item>
            the time for
            <ruleref uri="#EventMaybeTemporallyStampped"/>
          </item>
          <item>
            the purpose of
            <one-of>
              <item>
                <ruleref uri="#EventMaybeTemporallyStampped"/>
              </item>
              <item>this event</item>
            </one-of>
          </item>
          <item>
            <ruleref uri="#TemporalReference"/>'s
            event schedule
          </item>
          <item>
            <ruleref uri="#EventMaybeTemporallyStampped"/>
            about
          </item>
        </one-of>
      </item>
      <item>
        <ruleref uri="#EventCategory"/>
        are
        <ruleref uri="#TemporalReference"/>
      </item>
      <item>
        do I have
        <item repeat="0-1">for</item>
        <ruleref uri="#TemporalReference"/>
      </item>
    </one-of>
  </rule>

  <rule id="WhenQuestion">
    When
    <one-of>
      <item>
        does <ruleref uri="#Course"/> meet
      </item>
      <item>
        should I go to <ruleref uri="#EventMaybeTemporallyStampped"/>
      </item>
      <item>
        will
        <ruleref uri="#EventMaybeTemporallyStampped"/>
        <one-of>
          <item>start</item>
          <item>end</item>
        </one-of>
      </item>
      <item>
        is
        <one-of>
          <item>
            <ruleref uri="#EventMaybeTemporallyStampped"/>
          </item>
          <item>
            the next <ruleref uri="#EventName"/>
          </item>
        </one-of>
      </item>
    </one-of>
  </rule>


  <rule id="WhereQuestion">
    Where
    <one-of>
      <item>
        <one-of>
          <item>would</item>
          <item>can</item>
          <item>could</item>
        </one-of>
        I find
        <one-of>
          <item>
            <ruleref uri="#ObjectPossessedByPerson"/>
          </item>
          <item>
            <ruleref uri="#LocationOrPerson"/>
          </item>
        </one-of>
      </item>
      <item>
        is
        <one-of>
          <item>
            <ruleref uri="#LocationOrPerson"/>
          </item>
          <item>
            <ruleref uri="#ObjectPossessedByPerson"/>
          </item>
          <item>
            <ruleref uri="#EventMaybeTemporallyStampped"/>
            going to take place
          </item>
        </one-of>
      </item>
      <item>
        did
        <ruleref uri="#PersonReference"/>
        get
        <ruleref uri="#PossessivePronoun"/>
        <ruleref uri="#DegreeLevel"/>
      </item>
      <item>
        does
        <one-of>
          <item>
            <ruleref uri="#EventMaybeTemporallyStampped"/>meet
          </item>
          <item>
            <ruleref uri="#PersonReference"/> work
          </item>
        </one-of>
      </item>
    </one-of>
  </rule>

  <rule id="WhichQuestion">
    <item>
      Which
      <one-of>
        <item>
          <ruleref uri="#Email"/>
          of
          <ruleref uri="#PersonReference"/>
          do you have
        </item>
        <item>
          <one-of>
            <item>
              <item repeat="0-1">
                graduate
              </item>
              students are
            </item>
            <item>professors are</item>
            <item>faculties are</item>
          </one-of>
          in
          <ruleref uri="#AcademicDiscipline"/>
          <item repeat="0-1">group</item>
        </item>
        <item>
          days are <ruleref uri="#Course"/>
          <item repeat="0-1">offered</item>
        </item>
        <item>
          group is <ruleref uri="#PersonReference"/> in
        </item>
      </one-of>
    </item>
  </rule>

  <rule id="WhoQuestion">
    <one-of>
      <item>
        Who
        <one-of>
          <item>
            are
            <one-of>
              <item>
                the <ruleref uri="#AcademicDiscipline"/>
                <one-of>
                  <item>professors</item>
                  <item>faculties</item>
                </one-of>
              </item>
              <item>
                in <ruleref uri="#AcademicDiscipline"/>
              </item>
            </one-of>
          </item>
          <item>
            is
            <one-of>
              <item>
                <ruleref uri="#EventMaybeTemporallyStampped"/> for
              </item>
              <item>
                teaching
                <one-of>
                  <item>
                    <ruleref uri="#Course"/>
                    <item repeat="0-1">
                      <ruleref uri="#QuarterReference"/>
                    </item>
                  </item>
                  <item>
                    the introduction class in the <ruleref uri="#AcademicDiscipline"/> track
                  </item>
                </one-of>
              </item>
              <item>
                hosting <ruleref uri="#EventMaybeTemporallyStampped"/>
              </item>
              <item>
                in <ruleref uri="#AcademicDiscipline"/>
              </item>
              <item>
                <item repeat="0-1">the name of</item>
                the professor for <ruleref uri="#Course"/>
                <item repeat="0-1">
                  <ruleref uri="#QuarterReference"/>
                </item>
              </item>
            </one-of>
          </item>
          <item>
            does
            <one-of>
              <item>
                <one-of>
                  <item>work</item>
                  <item>research</item>
                </one-of>
                in <ruleref uri="#AcademicDiscipline"/>
              </item>
              <item>
                <ruleref uri="#AcademicDiscipline"/> research
              </item>
            </one-of>
          </item>
          <item>
            can teach me some knowledge of <ruleref uri="#AcademicDiscipline"/>
          </item>
          <item>
            should I talk with to get more information of
            <item repeat="0-1">the</item>
            <ruleref uri="#EventMaybeTemporallyStampped"/>
          </item>
          <item>
            works in <ruleref uri="#AcademicDiscipline"/>
          </item>
        </one-of>
      </item>
      <item>
        Whose class should I take to
        <one-of>
          <item>
            learn some introduction information of <ruleref uri="#AcademicDiscipline"/>
          </item>
          <item>
            learn some introductory information of <ruleref uri="#AcademicDiscipline"/>
          </item>
          <item>
            get an introduction of <ruleref uri="#AcademicDiscipline"/>
          </item>
        </one-of>
      </item>
    </one-of>
  </rule>

  <rule id="WillQuestion">
    Will
    <one-of>
      <item>
        <ruleref uri="#EventMaybeTemporallyStampped"/>
        be
        <one-of>
          <item>
            <ruleref uri="#Duration"/>
          </item>
          <item>
            <ruleref uri="#DurationComparison"/>
          </item>
          <item>long</item>
          <item>short</item>
        </one-of>
      </item>
    </one-of>
  </rule>


  <!-- returner -->














  <rule id="EnglishArticle">
    <one-of>
      <item>a</item>
      <item>an</item>
      <item>some</item>
      <item>any</item>
      <item>the</item>
    </one-of>
  </rule>
  <rule id="ThingsOfInterest">
    <one-of>
      <item>
        <ruleref uri="#ObjectsOfInterest"/>
      </item>
      <item>
        <ruleref uri="#LocationOrPerson"/>
      </item>
    </one-of>
  </rule>
  <rule id="LocationOrPerson">
    <one-of>
      <item>
        <ruleref uri="#PersonReference"/>
      </item>
      <item>
        <item repeat="0-1">the</item>
        <ruleref uri="#SpecificPlace"/>
      </item>
    </one-of>
  </rule>

  <rule id="Alumn">
    <one-of>
      <item>alumn</item>
      <item>alumnus</item>
      <item>alumni</item>
    </one-of>
  </rule>
  <rule id="School">
    <one-of>
      <item>
        <one-of>
          <item>Northwestern</item>
          <item>Carnegie Mellon</item>
          <item>Tufts</item>
        </one-of>
        <item repeat="0-1">University</item>
      </item>
      <item>Massechusetts Institute of Technology</item>
      <item>MIT</item>
      <item>CMU</item>
    </one-of>
  </rule>
  <rule id="Date">
    <item>
      <one-of>
        <item>
          <ruleref uri="#Month"/>
        </item>
        <item>the</item>
      </one-of>
      <ruleref uri="#OrdinalForDates"/>
    </item>
  </rule>
  <rule id="Month">
    <one-of>
      <item>January</item>
      <item>February</item>
      <item>March</item>
      <item>April</item>
      <item>May</item>
      <item>June</item>
      <item>July</item>
      <item>August</item>
      <item>September</item>
      <item>October</item>
      <item>November</item>
      <item>December</item>
    </one-of>
  </rule>
  <rule id="DayOfTheWeek">
    <one-of>
      <item>Monday</item>
      <item>Tuesday</item>
      <item>Wednesday</item>
      <item>Thursday</item>
      <item>Friday</item>
      <item>Saturday</item>
      <item>Sunday</item>
    </one-of>
  </rule>

  <rule id="OrdinalForDates">
    <one-of>
      <item>
        <ruleref uri="#OrdinalNumberFirstToNinth"/>
      </item>
      <item>
        <ruleref uri="#OrdinalNumberEleventhToNinteenth"/>
      </item>
      <item>
        <ruleref uri="#OrdinalNumberTenths"/>
      </item>
      <item>
        <ruleref uri="#TwentyToNinety"/>
        <ruleref uri="#OrdinalNumberFirstToNinth"/>
      </item>
    </one-of>
  </rule>

  <rule id="OrdinalNumberFirstToNinth">
    <one-of>
      <item>first</item>
      <item>second</item>
      <item>third</item>
      <item>fourth</item>
      <item>fifth</item>
      <item>sixth</item>
      <item>seventh</item>
      <item>eighth</item>
      <item>ninth</item>
    </one-of>
  </rule>
  <rule id="OrdinalNumberEleventhToNinteenth">
    <one-of>
      <item>eleventh</item>
      <item>twelfth</item>
      <item>thirteenth</item>
      <item>fourteenth</item>
      <item>fifteenth</item>
      <item>sixteenth</item>
      <item>seventeenth</item>
      <item>eighteenth</item>
      <item>nineteenth</item>
    </one-of>
  </rule>
  <rule id="OrdinalNumberTenths">
    <one-of>
      <item>tenth</item>
      <item>twentieth</item>
      <item>thirtieth</item>
      <item>fortieth</item>
      <item>fiftieth</item>
      <item>sixtieth</item>
      <item>seventieth</item>
      <item>eightieth</item>
      <item>ninetieth</item>
    </one-of>
  </rule>

  <rule id="SchoolLevel">
    <one-of>
      <item>graduate school</item>
      <item>college</item>
    </one-of>
  </rule>
  <rule id="DegreeLevel">
    <one-of>
      <item>PhD</item>
      <item>PhD Degree</item>
      <item>Master's Degree</item>
      <item>Bachelor's Degree</item>
    </one-of>
  </rule>
  <rule id="TimeStamp">
    <item>
      <ruleref uri="#OneToTwentyThree"/>
      <item repeat="0-1">
        <one-of>
          <item>o'clock</item>
          <item>o'five</item>
          <item>fifteen</item>
          <item>thirty</item>
          <item>fourty five</item>
        </one-of>
      </item>
      <item repeat="0-1">
        <one-of>
          <item>am</item>
          <item>pm</item>
        </one-of>
      </item>
    </item>
  </rule>
  <rule id="QuarterReference">
    <one-of>
      <item>this quarter</item>
      <item>next quarter</item>
      <item>
        <item repeat="0-1">
          <one-of>
            <item>this</item>
            <item>next</item>
          </one-of>
        </item>
        <item>fall quarter</item>
        <item>winter quarter</item>
        <item>spring quarter</item>
        <item>summar quarter</item>
      </item>
    </one-of>
  </rule>

  <rule id="ObjectCanBePossessed">
    <one-of>
      <item>office</item>
      <item>desk</item>
      <item>
        <ruleref uri="#Email"/>
      </item>
      <item>
        <ruleref uri="#Homepage"/>
      </item>
    </one-of>
  </rule>
  <rule id="ObjectPossessedByPerson">
    <one-of>
      <item>
        <ruleref uri="#PersonReference"/>'s
        <ruleref uri="#ObjectCanBePossessed"/>
      </item>
      <item>
        the
        <ruleref uri="#ObjectCanBePossessed"/>
        of
        <ruleref uri="#PersonReference"/>
      </item>
    </one-of>
  </rule>
  <rule id="Email">
    email
    <item repeat="0-1">address</item>
  </rule>
  <rule id="Homepage">
    <one-of>
      <item>webpage</item>
      <item>website</item>
      <item>homepage</item>
    </one-of>
    <item repeat="0-1"> address</item>
  </rule>

  <rule id="Place">
    <one-of>
      <item>
        <ruleref uri="#LocationReference"/>
      </item>
      <item>
        <ruleref uri="#SpecificPlace"/>
      </item>
    </one-of>
  </rule>

  <rule id="LocationReference">
    <one-of>
      <item>here</item>
      <item>there</item>
      <item>this building</item>
      <item>this floor</item>
    </one-of>
  </rule>

  <rule id="SpecificPlace">
    <one-of>
      <item>
        Ford<item repeat="0-1">
          <ruleref uri="#OfficeNumber"/>
        </item>
      </item>
      <item>QRG Lab</item>
      <item>
        Dean's
        <one-of>
          <item>conference room</item>
          <item>office</item>
        </one-of>
      </item>
      <item>empty room</item>
      <item>study room</item>
      <item>
        <ruleref uri="#PersonReference"/>'s
        <one-of>
          <item>office</item>
          <item>desk</item>
        </one-of>
      </item>
      <item>
        <item repeat="0-1">
          <ruleref uri="#EnglishArticle"/>
        </item>
        <item repeat="0-1">
          <one-of>
            <item>men's</item>
            <item>women's</item>
          </one-of>
        </item>
        <one-of>
          <item>bathroom</item>
          <item>toilet</item>
        </one-of>
      </item>
    </one-of>
  </rule>
  <rule id="Duration">
    <one-of>
      <item>thirty minutes</item>
      <item>forty five minutes</item>
      <item>an hour</item>
      <item>an hour and a half</item>
      <item>two hours</item>
      <item>three hours</item>
      <item>
        <item repeat="0-1">
          <one-of>
            <item>a</item>
            <item>a whole</item>
            <item>an entire</item>
          </one-of>
        </item>
        day
      </item>
    </one-of>
